                  <div id="panel04" class="tab-panel">
                    <div class="flexRow">
                      <h3>PROJECTS</h3>
                      <div class="flexRow">
                        <p>Want to showcase more Projects ?</p>
                        <button class="addExperience" id="addProject">
                          Click here
                        </button>
                      </div>
                    </div>
                    <section class="grid-item main">
                      <div class="items">
                        <% user.personalProjects.forEach((project) => {%>
                        <div class="item item1">
                          <div class="project-lang">
                            <div class="lang lang1">
                              <img
                                class="language"
                                src="/img/skill-icons/js.png"
                                alt=""
                              />
                            </div>
                            <div class="lang lang2">
                              <img
                                class="language"
                                src="/img/skill-icons/css.png"
                                alt=""
