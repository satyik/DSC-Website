    <%-include('partials/navbar')%>
    <!-- Edit Expereince Modal  -->
    <div class="addExperienceModal" id="addExperienceModal">
      <div class="wrapper backPart">
        <div id="subscribeBox">
          <img
            id="backlogo1"
            class="backlogo1"
            src="/img/logo/bac.png"
            alt=""
          />
        </div>
        <div></div>
      </div>
      <div class="wrapper">
        <div id="subscribeBox">
          <h2><span class="thin">Add a new </span> Experience</h2>

          <form
            class="subscribeForm"
            name="Subscription Form"
            method="post"
            action="/dsc/user/experience"
          >
            <div class="flexRow">
              <label for="exp_name"> Company Name</label>
              <input id="exp_name" type="text" name="exp_name" />
            </div>
            <div class="flexRow">
              <label for="exp_role"> Job Role</label>
              <input id="exp_role" type="text" name="exp_role" />
            </div>
            <div class="flexRow">
              <label for="exp_startdate">Start Date</label>
              <input
                id="startdate"
                type="text"
                placeholder="dd-mm-yy"
                name="exp_startdate"
                required
                
              />
            </div>
            <div class="flexRow">
              <label for="exp_enddate">End Date</label>
              <input
                id="exp_enddate"
                type="text"
                placeholder="dd-mm-yy"
                name="exp_enddate"
              />
            </div>

            <div class="flexRow">
              <label for="exp_status">Status</label>
              <select name="exp_status" id="Status">
                <option value="Ongoing">Ongoing</option>
                <option value="Scheduled">Scheduled</option>
                <option value="Completed">Completed</option>
              </select>
            </div>
            <div class="flexRow">
              <label for="exp_description">Description</label>
              <input id="Description" type="text" name="exp_description" />
            </div>
            <br /><br />
            <div class="flexRow fullwidth">
              <input id="submit" type="submit" value="Save Changes" />
            </div>
          </form>
        </div>
      </div>
    </div>
    <!-- Edit Expereince Modal  -->
    <!-- Edit Profile Modal  -->
    <div class="editProfileModal" id="editProfileModal">
      <div class="wrapper backPart">
        <div id="subscribeBox">
          <img id="backlogo" class="backlogo" src="/img/logo/bac.png" alt="" />
        </div>
        <div></div>
      </div>
      <div class="wrapper">
        <div id="subscribeBox">
          <h2><span class="thin">Profile</span> Details</h2>
          <form
            class="subscribeForm"
            name="Subscription Form"
            method="post"
            action="/dsc/user/profile"
          >
            <div class="flexRow">
              <label for="name">Name</label>
              <input
                id="name"
                type="text"
                value="<%=user.name%>"
                name="name"
                required
                maxlength="20"
              />
            </div>
            <div class="flexRow">
              <label for="hname">DSC Handle</label>
              <input
                id="hname"
                type="text"
                value="<% if(user.dscHandle!=null){%><%=user.dscHandle%><%}%>"
                name="hname"
                required
                maxlength="20"
              />
            </div>

            <div class="flexRow">
              <label for="degree">Degree</label>
              <select name="degree" id="degree" required>
                <option value="Bachelor of Technology" selected>
                  Bachelor of Technology
                </option>
                <option value="Master of Technology">
                  Master of Technology
                </option>
                <option value="PHD">PHD</option>
              </select>
            </div>
            <div class="flexRow">
              <label for="branch">Branch</label>
              <select name="branch" id="branch" required>
                <option value="Computer Science and Engineering">CSE</option>
                <option value="Electrical Engineering">EE</option>
                <option value="Electronics and Communication Engineering">
                  ECE
                </option>
                <option value="Civil Engineering">CE</option>
                <option value="Electronics & Instrumentation">ENI</option>
              </select>
            </div>
            <div class="flexRow">
              <label for="batchYear">Batch Year</label>
              <select name="batchYear" id="batchYear" required>
                <option value="2020-24">2020-24</option>
                <option value="2019-23" selected>2019-23</option>
                <option value="2018-22">2018-22</option>
                <option value="2017-21">2017-21</option>
              </select>
            </div>
            <div class="flexRow">
              <label for="bio">Bio</label>
              <input
                id="bio"
                type="text"
                placeholder="Something about you."
                name="bio"
                value="<% if(user.bio!=null){%><%=user.bio%><%}%>"
                required
                maxlength="50"
              />
            </div>
            <div class="flexRow">
              <label for="email">Email</label>
              <input
                id="email"
                type="email"
                value="<%= user.email %>"
                name="email"
                disabled
              />
            </div>
            <div class="flexRow">
              <label for="password">Password </label>
              <div class="flexRow fullwidth">
                <p>Last Changed 2 months ago</p>
                <a href="">Change Password</a>
              </div>
            </div>
            <br /><br />
            <div class="flexRow fullwidth">
              <input id="submit" type="submit" value="Save Changes" required />
              <a href="">Deactivae My Account</a>
            </div>
          </form>
        </div>
      </div>
      <div class="wrapper">
        <div id="subscribeBox">
          <div class="socials">
            <div class="items blue">
              <img src="/img/logo/fbw.jpg " alt="" />
              <a href="">Connect to Facebook</a>
            </div>
            <div class="items link">
              <img src="/img/logo/link.png" alt="" />
              <a href="">Connect to Linkedin</a>
            </div>
            <div class="items twitter">
              <img src="/img/logo/twitter.png" alt="" />
              <a href="">Connect to Twitter</a>
            </div>
          </div>
          <img class="edill" src="/img/illustrations/editing.png" alt="" />
        </div>
      </div>
    </div>
    <!-- Edit Profile Modal  -->

    <!-- Image Viewer Modal  -->
    <div class="modal">
      <img class="fullview" src="" alt="" />
    </div>
    <!-- Image Viewer Modal  -->

    <!-- addSkillsModal Modal  -->
    <div class="addSkillsModal" id="addSkillsModal">
      <div class="wrapper backPart">
        <div id="subscribeBox">
          <img
            id="backlogo2"
            class="backlogo1"
            src="/img/logo/bac.png"
            alt=""
          />
        </div>
        <div></div>
      </div>
      <div class="wrapper">
        <div id="subscribeBox">
          <h2><span class="thin">Add a new </span> Skills</h2>

          <form
            class="subscribeForm"
            name="Subscription Form"
            method="post"
            action="/dsc/user/skill"
          >
            <div class="flexRow">
              <label for="skillname">Skill Name</label>
              <input id="skillname" type="text" name="skillname" required />
            </div>
            <div class="flexRow">
              <label for="skilllevel">Skill Level</label>
              <input id="skilllevel" type="text" name="skilllevel" required />
            </div>
            <div class="flexRow">
              <label for="skillduration">Duration</label>
              <input
                id="skillsduration"
                placeholder="In months"
                name="skillduration"
                required
              />
            </div>
            <div class="flexRow">
              <label for="skilldescription">Description</label>
              <input
                id="skilldescription"
                type="text"
                name="skilldescription"
                required
              />
            </div>
            <br /><br />
            <div class="flexRow fullwidth">
              <input id="submit" type="submit" value="Save Changes" />
            </div>
          </form>
        </div>
      </div>
    </div>
    <!-- addSkillsModal Modal  -->

    <!-- addProjectsModal Modal  -->
    <div class="addProjectsModal" id="addProjectsModal">
      <div class="wrapper backPart">
        <div id="subscribeBox">
          <img
            id="backlogo3"
            class="backlogo1"
            src="/img/logo/bac.png"
            alt=""
          />
        </div>
        <div></div>
      </div>
      <div class="wrapper">
        <div id="subscribeBox">
          <h2><span class="thin">Add a new </span> Project</h2>

          <form
            class="subscribeForm"
            name="Subscription Form"
            method="post"
            action="/dsc/user/project/personal"
          >
            <div class="flexRow">
              <label for="projectname">Project Name</label>
              <input id="projectname" type="text" name="title" required />
            </div>
            <div class="flexRow">
              <label for="projectrole">Project Role</label>
              <input id="projectrole" type="text" name="role" required />
            </div>
            <div class="flexRow">
              <label for="startdate">Start Date</label>
              <input
                id="startdate"
                type="text"
                placeholder="dd-mm-yy"
                name="startdate"
                required
              />
            </div>
            <div class="flexRow">
              <label for="enddate">End Date</label>
              <input
                id="enddate"
                type="text"
                placeholder="dd-mm-yy"
                name="enddate"
                required
              />
            </div>
            <div class="flexRow">
              <label for="techstack">Techstacks</label>
              <select name="techstack" id="techstack" required>
                <option value="NWS">HTML, Vanilla JS</option>
                <option value="MEN">NodeJs, Mongoose, Express</option>
                <option value="MERN">ReactJs, Mongoose, Express</option>
                <option value="MEAN">AngularJs, Mongoose, Express</option>
                <option value="PWS">Python, DJango, Mongoose</option>
                <option value="NAS">Java, Firebase</option>
                <option value="FAS">Flutter, Firebase</option>
              </select>
            </div>
            <div class="flexRow">
              <label for="githuburl">Git Hub URL</label>
              <input
                id="githuburl"
                type="text"
                placeholder="Your Github repository link"
                name="githuburl"
                required
              />
            </div>
            <div class="flexRow">
              <label for="description">Description</label>
              <input id="description" type="text" name="description" required />
            </div>
            <br /><br />
            <div class="flexRow fullwidth">
              <input id="submit" type="submit" value="Save Changes" />
            </div>
          </form>
        </div>
      </div>
    </div>
    <!-- addSkillsModal Modal  -->
    <!-- addAchievementModal Modal  -->
    <div class="addAchievementModal" id="addAchievementModal">
      <div class="wrapper backPart">
        <div id="subscribeBox">
          <img
            id="backlogo4"
            class="backlogo1"
            src="/img/logo/bac.png"
            alt=""
          />
        </div>
        <div></div>
      </div>
      <div class="wrapper">
        <div id="subscribeBox">
          <h2><span class="thin">Add a new </span> Achievement</h2>

          <form
            class="subscribeForm"
            name="Subscription Form"
            method="POST"
            action="/dsc/user/achievement"
          >
            <div class="flexRow">
              <label for="achievementname">Achievement Name</label>
              <input
                id="achievementname"
                type="text"
                name="achievementname"
                required
              />
            </div>
            <div class="flexRow">
              <label for="achievementnameplatform">Achievement Platform</label>
              <input
                id="achievementnameplatform"
                type="text"
                name="achievementnameplatform"
                required
              />
            </div>
            <div class="flexRow">
              <label for="achievementnamedate">Received Date</label>
              <input
                id="achievementnamedate"
                type="text"
                placeholder="dd-mm-yy"
                name="achievementnamedate"
                required
              />
            </div>
            <div class="flexRow">
              <label for="achievementnameserial">Unique Serial ID</label>
              <input
                id="achievementnameserial"
                type="number"
                placeholder="This can be any unique value that indicated your Achievement."
                name="achievementnameserial"
                required
              />
            </div>
            <div class="flexRow">
              <label for="projectdescription">Description</label>
              <input
                id="projectdescription"
                type="text"
                name="projectdescription"
                required
              />
            </div>
            <br /><br />
            <div class="flexRow fullwidth">
              <input id="submit" type="submit" value="Save Changes" />
            </div>
          </form>
        </div>
      </div>
    </div>
    <!-- addAchievementModal Modal  -->
    <!-- Main Content Starts -->
    <div class="container">
      <section class="accountbar">
        <p></p>
        <div class="accountbar__shortcut">
          <div class="fndp" id="editbtn">Edit Profile</div>
          <a href="/dsc/user/logout" style="text-decoration: none">
            <div class="fndp">Log Out</div></a
          >
          <!-- <div class="edit" id="editbtn" ></div> -->
          <!-- <% if(user.profileImageLocation!=null){ %>
          <img
            class="bell resone"
            src="<%= user.profileImageLocation%>"
            alt="UserPhoto"
          />
          <% }else{%>

          <img
            class="bell resone"
            src="/img/logo/profile.png"
            alt="UserPhoto"
          />
          <% }%>

          <img class="bell" src="/img/logo/bell.png" alt="Notification Logo" /> -->
        </div>
      </section>
      <section class="accountdetails">
        <section class="dashboard__navbar">
          <ul class="side-nav">
            <!-- <li class="side-nav__item side-nav__item--active">
              <a href="#" class="side-nav__link">
                <span>Home</span>
              </a>
            </li>
            <li class="side-nav__item side-nav__item">
              <a href="#" class="side-nav__link">
                <span>Projects</span>
              </a>
            </li>
            <li class="side-nav__item side-nav__item">
              <a href="#" class="side-nav__link">
                <span>Achievements</span>
              </a>
            </li> -->
          </ul>
        </section>
        <section class="dashboard__main">
          <section class="main__profileinfo">
            <div class="pictureandbasic">
              <div class="photo">
                <% if(user.profileImageLocation!=null){ %>
                <img src=" <%= user.profileImageLocation%>" alt="" />
                <% }else{%> <img src="/img/logo/profile.png" alt="" /> <% }%>
              </div>
              <div class="info">
                <div class="nameandedit">
                  <h2><%= user.name %></h2>
                </div>
                <p>
                  <% if(user.branch!=null||user.batch!=null){ %> <%= user.degree
                  %> | <%= user.batch %> <br />
                  <%= user.branch %> <% }else{%> Your profile details are
                  empty.<% }%>
                </p>
                <p>
                  <% if(user.bio!=null){ %> <%= user.bio%> <% }else{%> <% }%>
                </p>
                <div class="branchandyear">
                  <div class="collegeinfo">
                    <% if(user.socialLinks["facebook"]!=null){ %>
                    <a href='<%= user.socialLinks["facebook"]%>'>
                      <img class="s" src="/img/logo/fb.png" alt="" />
                    </a>
                    <% }else{%>

                    <a href="#">
                      <img class="s" src="/img/logo/fb.png" alt="" /> </a
                    ><% }%>
                  </div>
                  <div class="collegeinfo">
                    <% if(user.socialLinks["linkedin"]!=null){ %>
                    <a href='<%= user.socialLinks["linkedin"]%>'>
                      <img class="s" src="/img/logo/link.png" alt="" />
                    </a>
                    <% }else{%>

                    <a href="#">
                      <img class="s" src="/img/logo/link.png" alt="" /> </a
                    ><% }%>
                  </div>
                  <div class="collegeinfo">
                    <% if(user.socialLinks["twitter"]!=null){ %>
                    <a href='<%= user.socialLinks["twitter"]%>'>
                      <img class="s" src="/img/logo/twitter.png" alt="" />
                    </a>
                    <% }else{%>

                    <a href="#">
                      <img class="s" src="/img/logo/twitter.png" alt="" /> </a
                    ><% }%>
                  </div>
                </div>
              </div>
              <div class="faint">
                <% if(user.dscHandle!=null){ %> <%= user.dscHandle%> <%}else{}%>
              </div>
            </div>
          </section>
          <section class="main__detailedinfo">
            <div class="panel-body">
              <div class="tab-container">
                <ul class="tab-buttons tab-buttons--basic tab-buttons--left">
                  <li class="tab-button-item" role="presentation">
                    <a
                      href="#panel01"
                      id="tab01"
                      class="tab-button tab-button--is-active"
                      data-a11ytab-tab
